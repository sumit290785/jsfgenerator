<?xml version="1.0" encoding="UTF-8"?>
<view xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="view.xsd">
	<tagtree id="entitypage">
		<statictag name="ui:composition">
			<statictag name="h:inputText">
				<attribute type="static" key="value" value="akarmi" />
			</statictag>
			<proxytag type="FORM" />
			
			<attribute type="xmlnamespace" value="h:http://java.sun.com/jsf/html" />
			<attribute type="xmlnamespace" value="ui:http://java.sun.com/jsf/facelets" />
			<attribute type="template" value="template.xhtml" />
		</statictag>
	</tagtree>
	
	<tagtree id="simpleform">
		<statictag name="table">
			<proxytag type="INPUT" />
		</statictag>
	</tagtree>
	
	<inputtag id="jsfgenerator.entitymodel.fields.TextFieldType">
		<statictag name="h:inputText">
			<attribute type="static" key="value" value="EXPRESSION"/>
		</statictag>
		
		<statictag name="h:inputTextArea">
			<attribute type="expression" key="value" value="EXPRESSION"/>
			<attribute type="static" key="cols" value="10"/>
		</statictag>
	
	</inputtag>
	
	<inputtag id="jsfgenerator.entitymodel.fields.NumberFieldType">
		<statictag name="h:inputText">
			<attribute type="expression" key="value" value="EXPRESSION"/>
		</statictag>
	</inputtag>
	
</view>